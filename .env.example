# FarmTech UP - Environment Variables
# Copy this file to .env and fill in your values

# Gemini API Key (optional)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Key for DALL-E image generation
OPENAI_API_KEY=your_openai_api_key_here

# GitHub configuration for automated publishing
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_REPO=your_username/farmtech-up

# Optional: GitHub Pages base URL
GITHUB_PAGES_URL=https://your_username.github.io/farmtech-up
